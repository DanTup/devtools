{
  "name": "flutter-tooling-extension-sample",
  "displayName": "flutter-tooling-extension-sample",
  "description": "An example VS Code extension that uses a Flutter UI and interacts with the Dart extension.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "extensionDependencies": [
    "Dart-Code.dart-code",
    "Dart-Code.flutter"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "flutter": [
        {
          "id": "dartFlutterToolingExtensionSample",
          "type": "webview",
          "name": "Tooling Extension Sample",
          "when": "dart-code:anyFlutterProjectLoaded && dart-code:isLsp"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "dart-code-api": "file:../dart_code_api_npm"
  }
}
